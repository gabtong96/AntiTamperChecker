<Window x:Class="AntiTamperChecker.InfoPage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AntiTamperChecker"
        mc:Ignorable="d"
        Title="InfoPage" Height="150" Width="300" ResizeMode="CanMinimize">
    <Grid Background="LightGray">
        <Grid.RowDefinitions>
            <RowDefinition Height="60"/>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>

        <Label Grid.Row="0" FontSize="25" FontWeight="Bold" Foreground="CadetBlue" HorizontalAlignment="Center">
            AntiTamperChecker 2.7
        </Label>

        <Label Grid.Row="1" FontSize="25" FontWeight="Bold" Foreground="CadetBlue" HorizontalAlignment="Center">
            Rohde &amp; Schwarz
        </Label>

    </Grid>
</Window>
